{
	"name": "@calmdownval/signal",
	"description": "A lightweight event dispatcher.",
	"version": "2.0.0",
	"license": "ISC",
	"type": "commonjs",
	"main": "./build/index.cjs.min.js",
	"module": "./build/index.esm.min.js",
	"types": "./build/index.d.ts",
	"exports": {
		".": {
			"require": "./build/index.cjs.min.js",
			"default": "./build/index.esm.min.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/CalmDownVal/signal.git"
	},
	"keywords": [
		"signal",
		"event",
		"async",
		"dispatcher"
	],
	"scripts": {
		"build": "rollup -c",
		"prepublishOnly": "rollup -c",
		"test": "ts-mocha -p ./test/tsconfig.json ./test/**/*.ts"
	},
	"devDependencies": {
		"@calmdownval/eslint-config": "CalmDownVal/eslint-config",
		"@calmdownval/eslint-import-resolver-typescript": "1.0.0",
		"@rollup/plugin-typescript": "8.2.1",
		"@types/mocha": "8.2.2",
		"@types/node": "15.6.1",
		"@types/sinon": "10.0.1",
		"@typescript-eslint/eslint-plugin": "4.23.0",
		"@typescript-eslint/parser": "4.23.0",
		"eslint": "7.26.0",
		"eslint-plugin-import": "2.22.0",
		"mocha": "8.4.0",
		"rollup": "2.50.3",
		"rollup-plugin-delete": "2.0.0",
		"rollup-plugin-dts": "3.0.2",
		"rollup-plugin-terser": "7.0.2",
		"sinon": "11.1.1",
		"ts-mocha": "8.0.0",
		"typescript": "4.3.2",
		"tslib": "2.2.0"
	}
}
